{
  "version": 2,
  "builds": [
    {
      "src": "pages/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pages/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/pages/dashboard.html"
    },
    {
      "src": "/admin",
      "dest": "/pages/admin.html"
    }
  ]
}